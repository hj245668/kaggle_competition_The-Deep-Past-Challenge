    "        )\n",
    "\n",
    "    batch_texts = tokenizer.batch_decode(outputs, skip_special_tokens=True)\n",
    "    decoded.extend([x.strip() for x in batch_texts])\n",
    "\n",
    "print(\"ì˜ˆì¸¡ ê°œìˆ˜:\", len(decoded))\n",
    "for i in range(min(3, len(decoded))):\n",

   "source": [
    "# 6. ì „ì²˜ë¦¬ ì ìš© ë° í•„í„°ë§\n",
    "\n",
    "train_df[\"transliteration\"] = train_df[\"transliteration\"].apply(normalize_akkadian)\n",
    "train_df[\"translation\"] = train_df[\"translation\"].apply(normalize_translation)\n",
    "test_df[\"transliteration\"] = test_df[\"transliteration\"].apply(normalize_akkadian)\n",
    "\n",
    "# ë…¸ì´ì¦ˆ ì œê±°\n",
    "train_df = train_df[\n",
    "    (train_df[\"transliteration\"].str.len() > 3) &\n",
    "    (train_df[\"translation\"].str.len() > 3)\n",
    "].reset_index(drop=True)\n",
    "\n",
    "print(train_df.shape)\n"

   "source": [
    "#5. ì „ì²˜ë¦¬ í•¨ìˆ˜ ì •ì˜\n",
    "\n",
    "def normalize_akkadian(text):\n",
    "    if pd.isna(text):\n",
    "        return \"\"\n",
    "    text = str(text)\n",
    "    text = re.sub(r'\\[([^\\]]*)\\]', r'\\1', text)\n",
    "    text = re.sub(r'<([^>]*)>', r'\\1', text)\n",
    "    text = re.sub(r'[â€¦â¸¢â¸£]', '', text)\n",
    "    text = re.sub(r'\\?+', '', text)\n",
    "    text = re.sub(r'-+', '-', text)\n",
    "    text = re.sub(r'\\s+', ' ', text)\n",
    "    return text.strip()\n",
    "\n",
    "def normalize_translation(text):\n",
    "    if pd.isna(text):\n",
    "        return \"\"\n",
    "    text = str(text)\n",
    "    text = re.sub(r'\\s+', ' ', text).strip()\n",
    "    if len(text) > 0:\n",
    "        text = text[0].upper() + text[1:]\n",
    "    return text\n"
   ]
  },
